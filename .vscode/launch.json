    "version": "0.2.0",
    "configurations": [

        {

            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SimpleXML_term/bin/Debug/netcoreapp2.1/SimpleXML_term.dll",
            "args": [],
            "cwd": "${workspaceFolder}/SimpleXML_term",
            "console": "integratedTerminal",
            "stopAtEntry": false

        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}